Package: contoso
Type: Package
Title: Dataset of the 'Contoso' Company
Version: 1.2.2
Authors@R: c(
    person(
      "Alejandro", "Hagan",
      email = "alejandro.hagan@outlook.com",
      role = c("aut", "cre")
    )
